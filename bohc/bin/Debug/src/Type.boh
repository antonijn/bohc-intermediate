package boh.lang;public abstract class Type{	private final String name;	private final Package pack;	protected cvisible this()	{	}		public abstract boolean isSubTypeOf(Type type);		public String getName()	{		return name;	}		public Package getPackage()	{		return pack;	}		public override String toString()	{		if (pack == Package.GLOBAL)		{			return name;		}				return pack.toString() + "." + name;	}}