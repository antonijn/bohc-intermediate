package boh.lang;public class Object{	public virtual String toString()	{		return getType().getName();	}		public virtual long hash()	{		return native.boh_force_cast(this);	}		public virtual Type getType()	{		return typeof(Object);	}		public virtual boolean equals(Object other)	{		return other r_eq this;	}		public static boolean valEquals(Object l, Object r)	{		boolean lNull = l r_eq null;		boolean rNull = r r_eq null;				if (lNull && rNull)		{			return true;		}		if (lNull || rNull)		{			return false;		}				return l.equals(r);	}}