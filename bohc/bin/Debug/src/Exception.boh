package boh.lang;public class Exception{	private String stackTrace;	private String description;	public this()	{	}		public String stackTrace()	{		return stackTrace;	}		public String getDescription()	{		return description;	}		public virtual String what()	{		return "Something went wrong in the application";	}		public this(String description)	{		this.description = description;	}		public override String toString()	{		return getType().toString() + "; " + what() + ": " + description + "\n" + stackTrace;	}}