package boh.lang;

public struct Vector3<T>
{
	public T x;
	public T y;
	public T z;
	
	public this(T x, T y, T z)
	{
		this.x = x;
		this.y = y;
		this.z = z;
	}
	
	public static Vector3<T> +(Vector3<T> left, Vector3<T> right)
	{
		return new Vector3<T>(left.x + right.x, left.y + right.y, left.z + right.z);
	}
}
